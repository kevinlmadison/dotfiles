! special
*.foreground:   #f8f8f2
*.background:   #272822
*.cursorColor:  #f8f8f2

! black
*.color0:       #272822
*.color8:       #75715e

! red
*.color1:       #f92672
*.color9:       #f92672

! green
*.color2:       #a6e22e
*.color10:      #a6e22e

! yellow
*.color3:       #f4bf75
*.color11:      #f4bf75

! blue
*.color4:       #66d9ef
*.color12:      #66d9ef

! magenta
*.color5:       #ae81ff
*.color13:      #ae81ff

! cyan
*.color6:       #a1efe4
*.color14:      #a1efe4

! white
*.color7:       #f8f8f2
*.color15:      #f9f8f5

URxvt.internalBorder:      0
URxvt.externalBorder:      0
!urxvt*transparent:true
!urxvt*shading: 10
*scrollBar:           off

URxvt*fading:              30
URxvt*termName:            rxvt-unicode
URxvt.perl-ext-common:     matcher,resize-font,foo

!URxvt.keysym.F11:          perl:fullscreen:switch
URxvt*imLocale:            en_US.UTF-8

URxvt.url-launcher:        /usr/bin/firefox
!URxvt.keysym.M-Delete:     perl:matcher:list
URxvt.colorUL:             #7780a1
