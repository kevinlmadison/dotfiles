!URxvt*font: xft:Terminess Powerline:size=12
URxvt*font:            xft:bitstream vera sans mono:pixelsize=13,xft:Inconsolata for Powerline:size=12
!*boldFont:            xft:Mono:pixelsize=11
!*italicFont:          xft:Mono:pixelsize=11:italic
!*boldItalicFont:      xft:Mono:pixelsize=11:bold:italic
*letterSpace:         -1


Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

